<Window x:Class="NyxGameLauncher.View.View"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:src="clr-namespace:NyxGameLauncher.ViewModel"
        xmlns:local="clr-namespace:NyxGameLauncher.View"
        mc:Ignorable="d"
        Title="View" Height="450" Width="800">
    <Window.DataContext>
        <src:GameViewModel/>
    </Window.DataContext>
    <Grid>
        <Button Command="{Binding Path=AddGameCommand}"></Button>
    </Grid>
</Window>
